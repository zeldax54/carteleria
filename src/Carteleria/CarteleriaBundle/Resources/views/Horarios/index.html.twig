{% extends '@layouts/startlayout.html.twig' %}


 {% block avanzu_head %}
     {% stylesheets
     '@carteleriaBundle/Resources/public/fullcalendar/dist/fullcalendar.css'  output='css/compiled/calendar.css' filter='yui_css'
     %}
     <link rel="stylesheet"  href="{{ asset_url }}" />
     {% endstylesheets %}


     {% stylesheets
     '@carteleriaBundle/Resources/public/fullcalendar/dist/fullcalendar.print.css'   output='css/compiled/calendar2.css' filter='yui_css'

     %}

     <link rel="stylesheet"  href="{{ asset_url }} " media="print" />
     {% endstylesheets %}



 {% endblock %}

{% block avanzu_page_subtitle  %}
    Horarios
{% endblock %}


{% block avanzu_page_content %}

    <div class="col-md-12">
        <div class="box box-primary">
            <div class="box-body no-padding">
                <!-- THE CALENDAR -->
                <div id="calendar"></div>
            </div>
            <!-- /.box-body -->
        </div>
        <!-- /. box -->
    </div>

    <textarea hidden id="jsontext">{{ jsondata }} </textarea>



{% endblock %}







    {% block avanzu_javascripts_inline  %}


        {% javascripts
        '@carteleriaBundle/Resources/public/moment/*.js' output='js/compiled/calendar.js' filter='yui_js'
        '@carteleriaBundle/Resources/public/fullcalendar/dist/*.js' output='js/compiled/calendar.js' filter='yui_js'
        '@carteleriaBundle/Resources/public/js/*' output='js/compiled/main.js' filter='yui_js'
        %}
        <script src="{{ asset_url }}"></script>
        {% endjavascripts %}

    {% endblock %}









